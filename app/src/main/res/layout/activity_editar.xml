<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Editar">

    <EditText
        android:id="@+id/editText_ID_id"
        android:layout_width="255dp"
        android:layout_height="50dp"
        android:layout_marginTop="55dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="number"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText_nome_id"
        android:layout_width="255dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/editText_ID_id" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="101dp"
        android:layout_height="50dp"
        android:layout_marginTop="55dp"
        android:gravity="center"
        android:text="ID"
        android:textColor="#FF000000"
        app:layout_constraintEnd_toStartOf="@+id/editText_ID_id"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText_Status_id"
        android:layout_width="255dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:text="Presente"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/editText_nome_id" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="101dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="HorÃ¡rio"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/editText_horario_id"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <Button
        android:id="@+id/button_edit_horario"
        android:layout_width="137dp"
        android:layout_height="66dp"
        android:layout_marginTop="25dp"
        android:drawableLeft="@drawable/ic_wall_clock"
        android:fontFamily="sans-serif-medium"
        android:text="Editar horario"
        android:textColor="#713B3B"
        android:textSize="13dp"
        app:backgroundTint="#FFF9C4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_edit"
        app:layout_constraintTop_toBottomOf="@+id/editText_Valor_id" />

    <EditText
        android:id="@+id/editText_Valor_id"
        android:layout_width="255dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="number"
        android:text=" 0,00"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/data_id" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="101dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Data"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/data_id"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="101dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Valor vendido"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/editText_Valor_id"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <Button
        android:id="@+id/button_view"
        android:layout_width="137dp"
        android:layout_height="66dp"
        android:layout_marginTop="5dp"
        android:drawableLeft="@drawable/ic_escala"
        android:fontFamily="sans-serif-medium"
        android:text="Escala"
        android:textColor="#713B3B"
        app:backgroundTint="#FFF9C4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_edit_value" />

    <EditText
        android:id="@+id/editText_horario_id"
        android:layout_width="255dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="number"
        android:text="00:00 / 00:00"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/editText_Status_id" />

    <Button
        android:id="@+id/button_edit_value"
        android:layout_width="137dp"
        android:layout_height="66dp"
        android:layout_marginTop="5dp"
        android:drawableLeft="@drawable/ic_dollar"
        android:fontFamily="sans-serif-medium"
        android:text="Editar valor"
        android:textColor="#713B3B"
        app:backgroundTint="#FFF9C4"
        app:layout_constraintEnd_toStartOf="@+id/button_edit_status"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_edit" />

    <Button
        android:id="@+id/button_edit"
        android:layout_width="137dp"
        android:layout_height="66dp"
        android:layout_marginTop="25dp"
        android:drawableLeft="@drawable/ic_editar_arquivo"
        android:fontFamily="sans-serif-medium"
        android:text="Editar Escala"
        android:textColor="#713B3B"
        app:backgroundTint="#FFF9C4"
        app:layout_constraintEnd_toStartOf="@+id/button_edit_horario"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="101dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Status"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/editText_Status_id"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <EditText
        android:id="@+id/data_id"
        android:layout_width="255dp"
        android:layout_height="50dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/editText_horario_id" />

    <Button
        android:id="@+id/button_edit_status"
        android:layout_width="137dp"
        android:layout_height="66dp"
        android:layout_marginTop="5dp"
        android:drawableLeft="@drawable/ic_check"
        android:fontFamily="sans-serif-medium"
        android:text="Editar Status"
        android:textColor="#713B3B"
        app:backgroundTint="#FFF9C4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button_edit_value"
        app:layout_constraintTop_toBottomOf="@+id/button_edit_horario" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="101dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Nome"
        android:textColor="#000000"
        app:layout_constraintEnd_toStartOf="@+id/editText_nome_id"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/bntVoltar"
        android:layout_width="55dp"
        android:layout_height="49dp"
        android:drawableLeft="@drawable/ic_home"
        android:textColor="#713B3B"
        app:backgroundTint="#FFF9C4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_view"
        app:layout_constraintVertical_bias="0.173" />

</androidx.constraintlayout.widget.ConstraintLayout>